{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "zinstall-init-packages": "npm i axios axios-mock-adapter font-awesome jquery lodash moment popper.js react-hook-form react-redux react-router-dom redux redux-logger redux-logger redux-mock-store redux-promise redux-saga redux-saga-test-plan sass yup",    "start": "npm run dev",
    "dev": "set REACT_APP_CONFIG=dev & react-scripts start",
    "pl1": "set REACT_APP_CONFIG=pl1 & react-scripts start",
    "pl2": "set REACT_APP_CONFIG=pl2 & react-scripts start",
    "prod": "set REACT_APP_CONFIG=prod & react-scripts start",
    "start0": "cls & clear & react-scripts start",
    "build": "set REACT_APP_CONFIG=prod & react-scripts build",
    "test": "set REACT_APP_CONFIG=test & react-scripts test --testURL=http://localhost:5000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.26.1",
    "axios-mock-adapter": "^1.20.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.2",
    "popper.js": "^1.16.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.29.0",
    "react-redux": "^7.2.8",
    "react-scripts": "5.0.0",
    "react-toastify": "^8.2.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-promise": "^0.6.0",
    "redux-saga": "^1.1.3",
    "redux-saga-test-plan": "^4.0.4",
    "sass": "^1.50.0",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  }
}
